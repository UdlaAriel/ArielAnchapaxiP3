<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ArielAnchapaxiP3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ArielAnchapaxiP3.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="ArielAnchapaxiP3">

    <FlyoutItem Title="Examen P3">
        <ShellContent
        Title="API Info"
        ContentTemplate="{DataTemplate local:APIInfoPage}"
        Route="APIInfoPage" />

        <ShellContent
        Title="Ariel Anchapaxi"
        ContentTemplate="{DataTemplate local:ArielAnchapaxi}"
        Route="ArielAnchapaxi" />
    </FlyoutItem>

</Shell>
